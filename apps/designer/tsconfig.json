{
  // Extends can't use node's resolution rn https://github.com/dividab/tsconfig-paths/issues/209#issuecomment-1177261091
  "extends": "../../node_modules/@webstudio-is/tsconfig/remix.json",
  "include": [
    "remix.env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "../../@types/**/*.d.ts"
  ],
  "compilerOptions": {
    "paths": { "~/*": ["app/*"] },
    "baseUrl": ".",
    "rootDir": "./app"
  }
}
