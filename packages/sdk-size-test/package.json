{
  "name": "@webstudio-is/sdk-size-test",
  "version": "0.22.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "size-test": "rm -rf ./public && rm -rf ./functions && remix build && size-limit",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "size-limit": [
    {
      "path": "public/build/*.js",
      "limit": "136 kB",
      "gzip": false
    },
    {
      "path": "functions/*.js",
      "limit": "238 kB",
      "gzip": false
    }
  ],
  "dependencies": {
    "@remix-run/cloudflare": "^1.18.1",
    "@remix-run/cloudflare-pages": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@webstudio-is/project": "workspace:^",
    "@webstudio-is/project-build": "workspace:^",
    "@webstudio-is/react-sdk": "workspace:^",
    "@webstudio-is/sdk-components-react": "workspace:^",
    "isbot": "^3.6.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.18.1",
    "@size-limit/file": "^8.2.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@webstudio-is/tsconfig": "workspace:^",
    "size-limit": "^8.2.4",
    "typescript": "5.1.6"
  }
}
