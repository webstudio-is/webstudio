{
  "name": "@webstudio-is/asset-uploader",
  "version": "0.3.0",
  "description": "Webstudio Asset Uploader",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "checks": "yarn typecheck && yarn lint && yarn test",
    "dev": "tsup --watch",
    "build": "rm -fr lib tsconfig.tsbuildinfo && tsc",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "publish-to-npm": "bash ../../bin/publish-to-npm.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.131.0",
    "@aws-sdk/lib-storage": "^3.131.0",
    "@webstudio-is/prisma-client": "*",
    "@webstudio-is/fonts": "*",
    "app-root-path": "^3.0.0",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/node": "^18.7.16",
    "@types/sharp": "^0.30.4",
    "@webstudio-is/jest-config": "*",
    "tsup": "^6.1.3",
    "typescript": "4.7.4",
    "zod": "^3.19.1"
  },
  "exports": {
    ".": "./lib/index.js",
    "./server": "./server.js"
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "README.md",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/index.server.ts"
    ],
    "format": "esm",
    "outDir": "lib"
  }
}
