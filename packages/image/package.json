{
  "name": "@webstudio-is/image",
  "version": "0.0.1",
  "description": "Image optimization",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "checks": "yarn typecheck && yarn lint && yarn test",
    "dev": "tsup --watch",
    "build:args": "generate-arg-types './src/*.tsx !./src/**/*.stories.tsx !./src/**/*.ws.tsx' && prettier --write \"**/*.props.json\"",
    "build": "rm -fr lib tsconfig.tsbuildinfo && tsc",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "storybook:run": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "generate-types": "generate-arg-types"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack4": "^6.5.6",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack4": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.13",
    "@webstudio-is/jest-config": "*",
    "typescript": "4.7.4",
    "@webstudio-is/generate-arg-types": "*"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "module": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./server": "./server.js"
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "README.md",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": "esm",
    "outDir": "lib"
  }
}
