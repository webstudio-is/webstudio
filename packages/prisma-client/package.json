{
  "name": "@webstudio-is/prisma-client",
  "version": "0.18.0",
  "description": "Webstudio Prisma layer",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "checks": "pnpm typecheck && pnpm lint",
    "dev": "pnpm concurrently 'prisma generate --watch' 'build-package --watch'",
    "build": "prisma format && prisma generate && build-package"
  },
  "bin": {
    "migrations": "./migrations-cli/cli.ts"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@webstudio-is/scripts": "workspace:^",
    "@webstudio-is/tsconfig": "workspace:^",
    "concurrently": "^7.6.0",
    "prisma": "^4.3.1",
    "typescript": "4.7.4",
    "zod": "^3.19.1"
  },
  "exports": "./lib/index.js",
  "types": "src/index.ts",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@inquirer/confirm": "^0.0.25-alpha.0",
    "@prisma/client": "^4.3.1",
    "@types/minimist": "^1.2.2",
    "dotenv": "^16.0.0",
    "execa": "^6.1.0",
    "minimist": "^1.2.6",
    "tsx": "^3.9.0",
    "umzug": "^3.2.1",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "zod": "^3.19.1"
  },
  "sideEffects": false
}
