{
  "name": "@webstudio-is/project",
  "version": "0.0.1",
  "description": "Webstudio Project",
  "author": "Webstudio <github@webstudio.is>",
  "homepage": "https://webstudio.is",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "checks": "yarn typecheck && yarn lint && yarn test",
    "dev": "tsup --watch",
    "build": "rm -fr lib tsconfig.tsbuildinfo && tsc",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0"
  },
  "dependencies": {
    "@webstudio-is/asset-uploader": "*",
    "@webstudio-is/prisma-client": "*",
    "@webstudio-is/react-sdk": "*",
    "bson-objectid": "^2.0.2",
    "immer": "^9.0.12",
    "nanoid": "^3.2.0",
    "slugify": "^1.6.5",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "tsup": "^6.1.3",
    "typescript": "4.7.4"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "README.md",
    "!*.test.*"
  ],
  "license": "MIT",
  "private": false,
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/index.server.ts"
    ],
    "format": "cjs",
    "outDir": "lib"
  }
}
